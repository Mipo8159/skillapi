{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/auth-lib": [
        "libs/auth-lib/src"
      ],
      "@app/auth-lib/*": [
        "libs/auth-lib/src/*"
      ],
      "@app/common": [
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*"
      ],
      "@app/config": [
        "libs/config/src"
      ],
      "@app/config/*": [
        "libs/config/src/*"
      ],
      "@app/health": [
        "libs/health/src"
      ],
      "@app/health/*": [
        "libs/health/src/*"
      ],
      "@app/logger": [
        "libs/logger/src"
      ],
      "@app/logger/*": [
        "libs/logger/src/*"
      ],
      "@app/resource-lib": [
        "libs/resource-lib/src"
      ],
      "@app/resource-lib/*": [
        "libs/resource-lib/src/*"
      ],
      "@app/tokens-lib": [
        "libs/tokens-lib/src"
      ],
      "@app/tokens-lib/*": [
        "libs/tokens-lib/src/*"
      ],
      "@app/users-lib": [
        "libs/users-lib/src"
      ],
      "@app/users-lib/*": [
        "libs/users-lib/src/*"
      ]
    }
  }
}