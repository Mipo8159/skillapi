{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/auth-lib": "<rootDir>/../libs/auth-lib/src",
    "@app/auth-lib/(.*)": "<rootDir>/../libs/auth-lib/src/$1",
    "@app/common": "<rootDir>/../libs/common/src",
    "@app/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@app/config": "<rootDir>/../libs/config/src",
    "@app/config/(.*)": "<rootDir>/../libs/config/src/$1",
    "@app/health": "<rootDir>/../libs/health/src",
    "@app/health/(.*)": "<rootDir>/../libs/health/src/$1",
    "@app/logger": "<rootDir>/../libs/logger/src",
    "@app/logger/(.*)": "<rootDir>/../libs/logger/src/$1",
    "@app/resource-lib": "<rootDir>/../libs/resource-lib/src",
    "@app/resource-lib/(.*)": "<rootDir>/../libs/resource-lib/src/$1",
    "@app/tokens-lib": "<rootDir>/../libs/tokens-lib/src",
    "@app/tokens-lib/(.*)": "<rootDir>/../libs/tokens-lib/src/$1",
    "@app/users-lib": "<rootDir>/../libs/users-lib/src",
    "@app/users-lib/(.*)": "<rootDir>/../libs/users-lib/src/$1"
  }
}